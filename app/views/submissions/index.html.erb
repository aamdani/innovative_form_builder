<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Custom Form Name</th>
      <th>Submitted by</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td><%= submission.custom_form.name %></td>
        <td><%= submission.user.email %></td>
        <td><%= submission.created_at.strftime("%b %d, %Y") %></td>
        <td><%= link_to 'Show', submission %></td>
        <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
        <td><%= link_to 'Destroy', submission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
