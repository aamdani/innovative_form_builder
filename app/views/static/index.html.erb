              <!-- Heroicon name: menu -->
              <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16" />
              </svg>
            </button>
          </div>
        </div>
        <div class="hidden space-x-10 md:flex md:ml-10">
          <a href="<%= home_target %>" class="font-medium text-white hover:text-builder-300">Dashboard</a>

          <a href="#features" class="font-medium text-white hover:text-builder-300">Features</a>

          <a href="#pricing" class="font-medium text-white hover:text-builder-300">Pricing</a>

          <a href="#company" class="font-medium text-white hover:text-builder-300">Company</a>
