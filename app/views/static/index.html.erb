<% if user_signed_in? %>
<% home_target = custom_forms_path %>
<% else %>
<% home_target = "#home" %>
<% end %>
<!-- Hero Section -->
<div id="hero" class="relative bg-builder-800 overflow-hidden">
  <div class="hidden sm:block sm:absolute sm:inset-0" aria-hidden="true">
